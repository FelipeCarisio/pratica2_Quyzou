    <center>
        <p id="tituloLogECadastro">Cadastrar</p>
    </center>

    <center>
        <form action="/Cadastro/Cadastrar" method="post">
            <div id="cadastro">

                <div class="row">
                    <div class="col s12">
                        <div class="row">
                            <div class="input-field col s6">
                                <input id="primeiro_nome" type="text" class="validate" name="u.Nome" required>
                                <label for="primeiro_nome">Primeiro nome</label>
                                <span class="helper-text" data-error="Errado" data-success="Correto">Nome até 30 caracteres</span>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col s12">
                        <div class="row">
                            <div class="input-field col s6">
                                <input id="email" type="email" class="validate" name="u.Email" required>
                                <label for="email">E-mail</label>
                                <span class="helper-text" data-error="Errado" data-success="Correto">E-mail no formato 'xxxxx@xxx.xxx'</span>
                            </div>                         
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col s12">
                        <div class="row">
                            <div class="input-field col s6">
                                <input id="senha" type="password" class="validate" name="u.Senha" required>
                                <label for="senha">Senha</label>
                                <span class="helper-text" data-error="Errado" data-success="Correto">Senha até 20 caracteres</span>
                            </div>
                        </div>
                    </div>
                </div>
                <button type="submit" class="waves-effect waves-light btn" href="login.html" id="btnFimLogin">Finalizar</button>
            </div>
          </form>
    </center>
    <br>